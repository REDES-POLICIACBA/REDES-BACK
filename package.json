{
	"name": "back",
	"version": "1.0.0",
	"main": "app.ts",
	"type": "module",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "nodemon ./src/app.ts",
		"prepare": "husky",
		"format": "biome format --write ./src",
		"lint": "npm run format && biome lint ./src --apply --no-errors-on-unmatched",
		"build": "npm run build:ts",
		"build:ts": "tsc",
		"lint-staged": "lint-staged"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"build": "^0.1.4",
		"cors": "^2.8.5",
		"cron": "^3.1.7",
		"dotenv": "^16.4.5",
		"expo-server-sdk": "^3.11.0",
		"express": "^4.19.2",
		"express-rate-limit": "^7.4.0",
		"firebase": "^10.13.1",
		"firebase-admin": "^12.2.0",
		"google-auth-library": "^9.14.0",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.4.4",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"node-cron": "^3.0.3",
		"nodemailer": "^6.9.14",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"socket.io": "^4.7.5",
		"ts-node": "^10.9.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/mongoose": "^5.11.97",
		"@types/morgan": "^1.9.9",
		"@types/multer": "^1.4.11",
		"@types/node-cron": "^3.0.11",
		"@types/nodemailer": "^6.4.15",
		"@types/passport": "^1.0.16",
		"@types/passport-jwt": "^4.0.1",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome format --write --no-errors-on-unmatched",
			"biome lint --apply --no-errors-on-unmatched",
			"biome check --files-ignore-unknown=true",
			"biome check --apply --no-errors-on-unmatched",
			"biome check --apply --organize-imports-enabled=false --no-errors-on-unmatched",
			"biome check --apply-unsafe --no-errors-on-unmatched"
		]
	},
	"keywords": [],
